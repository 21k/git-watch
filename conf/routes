GET         /                              @controllers.Main.index
POST        /github/                       @controllers.GitHub.push
GET         /github/:owner/:repo           @controllers.GitHub.watch(owner: String, repo: String)
POST        /bitbucket/                    @controllers.GitHub.push
GET         /bitbucket/:owner/:repo        @controllers.GitHub.watch(owner: String, repo: String)
GET         /github-callback/              @controllers.InstallHooks.githubCallback
GET         /install/                      @controllers.InstallHooks.installGet
POST        /install/                      @controllers.InstallHooks.installPost
GET         /static/*name                  @controllers.StaticDev.get(name: String)
GET         /version/                      @controllers.Version.version

